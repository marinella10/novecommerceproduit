/*
 * Buttons Counter  v1.0
 *
 * Copyright 2017, Sergi Minguez,
 */
(function($) {

    function ButtonsCounter(element, options) {
        this.root = $(element);

        var self = this;

        this.settings = $.extend({}, $.fn.buttonsCounter.defaults, options);

		if(this.settings.initValue == undefined) {
			this.counter = this.root.find(".field input").val();
		} else {
			this.counter = this.settings.initValue;
		}

        this.init();

        this.root.find('#up').click($.proxy(this._up, this));
        this.root.find('#down').click($.proxy(this._down, this));

        $(window).resize($.proxy(this._resize, this));
        $(window).load($.proxy(this._onload, this));
    }


    ButtonsCounter.prototype = {

		el: null,

        init: function() {
			this.el = this.root;

            this.root.find(".field input").val(this.counter);

            //fill mobile select
            this.root.find("select").empty();
            for (var i = this.settings.minValue; i <= this.settings.maxValue; i++) {
                this.root.find("select").append('<option value="' + i + '">' + i + '</option>');
            }

            this.root.find("select").val(this.counter);
            this.root.find("select").change($.proxy(this._onSelectChange, this));
        },

        _onSelectChange: function(event) {
            this.counter = this.root.find("select").val();
            this.root.find(".field input").val(this.counter);

            if (this.settings.onCounterChange !== undefined) {
				this.settings.onCounterChange(this.counter, this.root);
			}
        },

        _up: function(event) {
            event.preventDefault();

            this.counter++;
            if (this.counter > this.settings.maxValue) {
                this.counter = this.settings.maxValue;
            }

            this.root.find(".field input").val(this.counter);
            this.root.find("select").val(this.counter);

            if (this.settings.onCounterChange !== undefined)
                this.settings.onCounterChange(this.counter, this.root);
        },

        _down: function(event) {
            event.preventDefault();

            this.counter--;
            if (this.counter < this.settings.minValue)
                this.counter = this.settings.minValue;

            this.root.find(".field input").val(this.counter);
            this.root.find("select").val(this.counter);

            if (this.settings.onCounterChange !== undefined)
                this.settings.onCounterChange(this.counter, this.root);
        },


        _resize: function() {

        },

    }; /* ButtonsCounter.prototype end */

    $.fn.buttonsCounter = function(options) {
        return this.each(function() {
            var buttonsCounter = new ButtonsCounter($(this), options);
            $(this).data("buttonsCounter", buttonsCounter);
        });
    };


    $.fn.buttonsCounter.defaults = {
        //define defaults
        minValue: 0,
        maxValue: 10,
    };

    $.fn.buttonsCounter.settings = {};

})(jQuery);

/*! Magnific Popup - v1.1.0 - 2016-02-20
